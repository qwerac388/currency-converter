{
  "manifest_version": 3,

  "name": "Currency-Converter",
  "description": "Group 2 Project",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": "money.png"

  },
  "web_accessible_resources": [
    {
      "resources": ["images/*"],
      "matches": ["https://www.google.com/*"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["https://*/", "https://*/*", "http://*/"],
      "js": ["index.js"]
    }
  ]
}
